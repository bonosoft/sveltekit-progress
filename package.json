{
	"name": "@bonosoft/sveltekit-progress",
	"description": "Progress component for SvelteKit",
	"version": "0.0.1",
	"author": "Bonosoft",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/bonosoft/sveltekit-progress.git"
	},
	"bugs": {
		"url": "https://github.com/bonosoft/sveltekit-progress/issues"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"progress",
		"percent",
		"dashboard",
		"status",
		"gauge",
		"pressure",
		"circle",
		"circular",
		"rounded",
		"square",
		"donut",
		"risk"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"publish": "npm publish -access public"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^2.0.0",
		"publint": "^0.1.9",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.2.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
